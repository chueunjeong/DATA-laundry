<?xml version="1.0" encoding="UTF-8" ?> <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="data.laundry.mapper.CodeMapper">
	<select id="findAll" resultType="Code"> 
		select * from CODE
		
	</select>


	<select id="findByGroup_code_id" resultType="Code"> 
	select * from CODE 
		where group_code_id =#{group_code_id}
	</select>




</mapper>